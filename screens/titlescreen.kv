#:kivy 2.0.0
#:import BackGround1 backgrounds.background3

<TitleScreen>:
    BackGround3:
    RelativeLayout:
        Label:
            id:title_tap
            text:"Double Tap to Start"
            font_size: (self.height + self.width) / 10
            bold:True
            size_hint: .15 + root.sz_offset, .05 + root.sz_offset
            pos_hint: {'center_x': .5, 'center_y': .1}
        
